{
  "name": "champagnefestival",
  "private": true,
  "version": "0.1.0",
  "description": "Champagne Festival website using React and Bootstrap",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:react": "vite",
    "build": "next build",
    "build:react": "vite build",
    "start": "next start",
    "preview:react": "vite preview",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "deploy:static": "wrangler pages deploy out",
    "deploy:react": "npm run build:react && wrangler pages deploy dist"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "leaflet": "^1.9.4",
    "next": "^15.2.0",
    "next-intl": "^3.26.5",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.4.1",
    "react-leaflet": "^5.0.0-rc.2",
    "react-router-dom": "^6.22.0",
    "swiper": "^11.2.6"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.10",
    "@eslint/eslintrc": "^3.3.0",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^5.2.0",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.0",
    "typescript": "^5.7.3",
    "vite": "^5.3.1",
    "wrangler": "^4.0.0"
  }
}
